
! xrdb ~/.Xresources

urxvt.scrollBar:            false
urxvt.cursorBlink:          true
urxvt.cursorColor:          Magenta

URxvt.font: xft:Droid Sans Mono Nerd Font:style=Regular:size=11


! fix underscore not showing
urxvt.lineSpace:            1

! remap copy & paste
urxvt.keysm.Shift-Control-V: eval:paste_clipboard
urxvt.keysm.Shift-Control-P: eval:selection_to_clipboard

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
